{
  "openapi": "3.0.1",
  "info": {
    "title": "DDAC Health Login API",
    "version": "v1"
  },
  "paths": {
    "/login/register": {
      "post": {
        "tags": [
          "Login"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterVO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterVO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterVO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/HttpVO"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpVO"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpVO"
                }
              }
            }
          }
        }
      }
    },
    "/login/login": {
      "post": {
        "tags": [
          "Login"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginVO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginVO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginVO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/HttpVO"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpVO"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpVO"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HttpVO": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "message": {
            "type": "string",
            "nullable": true
          },
          "data": {
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginVO": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RegisterVO": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "password2": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}